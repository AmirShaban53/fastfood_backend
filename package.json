{
  "name": "fastfood_backend",
  "version": "1.0.0",
  "description": "backend of fast food web app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "set NODE_ENV=test&& mocha --timeout 10000 --exit",
    "ci:test": "set NODE_ENV=test&& mocha --timeout 10000 --ci --exit",
    "start": "nodemon -e yaml index.js",
    "coverage": "nyc npm run test "
  },
  "nyc": {
    "useSpwanWrap": true,
    "instrument": true,
    "cache": true,
    "all": true,
    
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ]
  },
  "author": "AMIR BUDDA SHABAN",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "dotenv": "^10.0.0",
    "mocha": "^9.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.20"
  }
}
